# Example configuration for DaemonSet
app_name = "simple-typescript-app"
app_version = "v1.0.0"
workload_type = "daemonset"
container_port = 3000
environment = "production"
node_port = 30000

resource_limits = {
  cpu    = "200m"
  memory = "256Mi"
}

resource_requests = {
  cpu    = "100m"
  memory = "128Mi"
}

labels = {
  environment = "production"
  workload    = "daemonset"
}

namespace = "default"
